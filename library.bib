issn = {15662543},
journal = {Journal of Polymers and the Environment},
keywords = {3D printing,Filament,HDPE,Plastic,Polymer,Recycling},
number = {2},
pages = {136--145},
publisher = {Springer US},
title = {{Physical Characterization and Pre-assessment of Recycled High-Density Polyethylene as 3D Printing Material}},
volume = {25},
