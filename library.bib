title = {{Three-dimensional Printed Acrylonitrile Butadiene Styrene Framework Coated with Cu-BTC Metal-organic Frameworks for the Removal of Methylene Blue}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4123204{\&}tool=pmcentrez{\&}rendertype=abstract{\%}5Cnhttp://www.nature.com/articles/srep05939},
