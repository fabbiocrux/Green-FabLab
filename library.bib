file = {:Users/fabio/Dropbox/Doctorado/Bibliography/Recyclage/Materiales/PET/Poly (ethylene terephthalate) thermo-mechanical and thermo-oxidative degradation mechanisms.pdf:pdf},
isbn = {0141-3910},
issn = {01413910},
journal = {Polymer Degradation and Stability},
keywords = {Diethylene glycol,Poly(ethylene terephthalate),Thermo-mechanical degradation,Thermo-oxidative degradation},
number = {10},
pages = {1849--1859},
title = {{Poly (ethylene terephthalate) thermo-mechanical and thermo-oxidative degradation mechanisms}},
volume = {94},
year = {2009}
}
