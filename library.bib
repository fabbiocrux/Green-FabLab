journal = {Virtual and Physical Prototyping},
keywords = {Benchmarking,Process Improvement,additive manufacturing,desktop 3D printer,material extrusion,sheet lamination,vat photopolymerisation},
mendeley-tags = {Benchmarking,Process Improvement},
month = {sep},
