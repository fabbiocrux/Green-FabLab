file = {:Users/fabio/Dropbox/Doctorado/Bibliography/Additive Manufacturing/Sustainability AM/Circular Economy/Unlocking value for a circular economy through 3D printing A research agenda.pdf:pdf},
isbn = {9788578110796},
issn = {00401625},
