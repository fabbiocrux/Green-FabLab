@article{Le2014,
abstract = {Modeling the structure and evolution of products is important from the standpoint of improving quality and maintainability. With the increasing popularity of open-source processes for developing both software and physical systems, there is a need to develop computational models of product evolution in such dynamic product developments scenarios. Existing studies on the evolution of products involve modeling products as networks, taking snapshots of the structure at different time steps, and comparing the structural characteristics. Such approaches are limited because they do not capture the underlying dynamics through which products evolve. In this paper, we take a step toward addressing this gap by presenting a generative network model for product evolution. The generative model is based on different mechanisms though which networks evolve—addition and removal of nodes, addition and removal of links. The model links local network observations to global network structures. It is utilized for modeling and analyzing the evolution of a software product (Drupal) and a physical product (RepRap) developed by open source processes. For the software product, the generated networks are compared with the actual product structures using various network measures including average degree, density, clustering coefficients, average shortest path, propagation cost, clustered cost, and degree distributions. For the physical product, the product evolution is analyzed in terms of the proposed mechanisms. The proposed model has three general applications: longitudinal studies of a product's evolution, cross-sectional studies of evolution of different products, and predictive analyzes.},
annote = {Goal:

Propose a generative model for product evolution of a software and physical product. The proposed model has three general applications: Longitudin


Conclusion:

The model captures the underlying dynamics of evolution of open surce software products.
The model can help in the identification not only the extent of increase in complexity over time but also the mechanisms through which the complexity increases.},
author = {Le, Qize and Sha, Zhenghui and Panchal, Jitesh H},
doi = {10.1115/1.4025856},
file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Le, Sha, Panchal - 2014 - A Generative Network Model for Product Evolution.pdf:pdf},
issn = {1530-9827},
journal = {Journal of Computing and Information Science in Engineering},
keywords = {Prospective,complex networks,degree-based models,evolution,product structure},
mendeley-tags = {Prospective},
month = {jan},
number = {1},
pages = {011003},
title = {{A Generative Network Model for Product Evolution}},
url = {http://dx.doi.org/10.1115/1.4025856 http://computingengineering.asmedigitalcollection.asme.org/article.aspx?doi=10.1115/1.4025856},
volume = {14},
year = {2014}
