keywords = {Agricultural plastic waste,Mechanical recycling,Recycling quality of plastic waste,Recycling technical specifications},
number = {6},
pages = {1516--1530},
pmid = {23561796},
publisher = {Elsevier Ltd},
title = {{Technical specifications for mechanical recycling of agricultural plastic waste}},
