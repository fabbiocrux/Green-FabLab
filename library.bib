author = {King, Debbie L. and Babasola, Adegboyega and Rozario, Joseph and Pearce, Joshua M.},
doi = {10.12924/cis2014.02010018},
file = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/King et al. - 2014 - Mobile Open-Source Solar-Powered 3-D Printers for Distributed Manufacturing in Off-Grid Communities.pdf:pdf},
issn = {2297-6477},
journal = {Challenges in Sustainability},
