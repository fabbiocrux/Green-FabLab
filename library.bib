month = {jan},
pages = {253--259},
publisher = {Elsevier Ltd},
title = {{Investigation for dimensional accuracy of AMC prepared by FDM assisted investment casting using nylon-6 waste based reinforced filament}},
url = {http://dx.doi.org/10.1016/j.measurement.2015.10.016 http://linkinghub.elsevier.com/retrieve/pii/S0263224115005503},
