issn = {1573-4838},
journal = {Journal of materials science. Materials in medicine},
keywords = {Biocompatible Materials,Biocompatible Materials: chemistry,Chemical,Computer Simulation,Hardness,Hot Temperature,Materials Testing,Models,Polyesters,Polyesters: chemistry,Rheology,Rheology: methods,Viscosity},
