@article{Kianian2015,
abstract = {This paper investigates how Additive Manufacturing (AM) technologies, as a process innovation, may contribute to a job creation. Further, the various mechanisms in which AM may contribute to an increase in job creation as well as the types of jobs are analyzed. The analysis also goes beyond AM technologies and incorporates other non-technological factors which foster job creation, i.e. higher wages in BRIC countries, lower quality in BRIC countries, and a rising demand for western-made products. The analysis is based on a case study and the data collected was through interviews with three prominent actors within the AM technologies field in Sweden: technology developers, leading suppliers and users. The main findings indicate that AM (i) contributes to job creation in both the manufacturing sector and in the service sector, (ii) does not bring back mass production jobs from emerging economies such as BRIC, (iii) contributes to job creation in product development stages (e.g. rapid prototyping), and (iv) contributes to job creation in production stages of low-volume batches mainly of complex products. The findings also suggest there are barriers for full exploitation of AM in several areas, including education systems.},
author = {Kianian, Babak and Tavassoli, Sam and Larsson, Tobias C.},
doi = {10.1016/j.procir.2014.07.109},
file = {:Users/fabio/Dropbox/Doctorado/Bibliography/3 Revolution Industrial/The Role of Additive Manufacturing Technology in job creation- an exploratory case study of suppliers of Additive Manufacturing in Sweden.pdf:pdf},
isbn = {2212-8271},
issn = {22128271},
journal = {Procedia CIRP},
keywords = {3D printing,Additive manufacturing technologies,Exploratory case study,Job creation,Sweden},
