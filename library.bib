journal = {Sustainability Science},
keywords = {Circular economy,Environmental economics,Externalities,Sustainability},
number = {1},
pages = {133--140},
pmid = {10299492},
title = {{An introductory note on the environmental economics of the circular economy}},
volume = {2},
year = {2007}
}
