journal = {Advances in polymer science},
keywords = {chromatography,degree of degradation,dsc,low molecular weight compounds,mae,multivariate modelling,nir,polymer composition,polyolefins,quality,recycling,recycling as resources},
pages = {201--230},
title = {{Recycled Polyolefins. Material Properties and Means for Quality Determination}},
url = {http://link.springer.com/chapter/10.1007/b94173{\%}5Cnhttp://www.springerlink.com/index/10.1007/b94173},
volume = {169},
year = {2004}
}
