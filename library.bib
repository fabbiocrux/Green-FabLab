journal = {Challenges in Sustainability},
keywords = {0,2014 by the authors,3,3-d printing,additive layer manufacturing,attribution license,by,creativecommons,development,distributed manufacturing,eye care,glasses,http,licensee librello,licenses,org,switzerland,this open access article,under a creative commons,was published},
number = {1},
pages = {30--40},
title = {{Reversing the Trend of Large Scale and Centralization in Manufacturing: The Case of Distributed Manufacturing of Customizable 3-D-Printable Self-Adjustable Glasses}},
url = {https://scholar.google.es/scholar?hl=en{\&}q=Reversing+the+Trend+of+Large+Scale+and+Centralization+in+Manufacturing:+The+Case+of+Distributed+Manufacturing+of+Customizable+3-D-Printable+Self-Adjustable+Glasses{\&}btnG={\&}as{\_}sdt=1,5{\&}as{\_}sdtp=},
