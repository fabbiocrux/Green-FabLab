journal = {Journal of Sustainable Development},
keywords = {Applications,Sustainability,appropriate technology,local manufacturing,open design,open source,open source appropriate,peer to peer production,self-reliance,sustainable development,technology},
mendeley-tags = {Applications,Sustainability},
month = {nov},
